# PaperForge Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Configuration (for real implementation)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# OpenRouter Configuration (alternative to OpenAI)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# Application Configuration
VITE_APP_NAME=PaperForge
VITE_APP_VERSION=1.0.0

# API Base URLs
VITE_API_BASE_URL=https://api.paperforge.ai
VITE_ARXIV_API_URL=https://export.arxiv.org/api/query

# Feature Flags
VITE_ENABLE_BENCHMARKING=true
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_API_ACCESS=false

# Analytics (optional)
VITE_ANALYTICS_ID=your_analytics_id_here